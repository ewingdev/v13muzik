     "required": true
    },
    "NODE_HOST":{
      "description":"lavalink host",
      "value": "disbotlistlavalink.ml",
      "required": true
    },
    "NODE_ID":{
      "description":"lavalink ID",
      "value": "main",
      "required": true
    },
    "NODE_PORT":{
      "description":"lavalink port",
      "value": "443",
      "required": true
    },
    "NODE_PASSWORD":{
      "description":"lavalink password",
      "value": "LAVA",
      "required": true
    },
    "NODE_SECURE":{
      "description":"lavalink secure (true/false)",
      "value": "true",
      "required": true
    },
    "PREFIX":{
      "description": "your prefix",
      "value": "!",
      "required": true
     },
    "COlOR":{
      "description": "embed colour code hex",
      "value": "#660099",
      "required": false
     },
    "LOGS":{
      "description": "LOG KANALI İD",
      "required": true
     },
    "OWNERID":{
      "description": "İDNİZ",
      "required": true
     },
    "TOKEN": {
      "description": "TOKENİNİZ",
      "required": true
    }
  }
