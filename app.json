  "logo": "https://cdn.discordapp.com/attachments/936263362185166848/936273646710829156/ewing_1.png",
  "keywords": ["discord", "discordjs", "music"],
  "env": {    
    "MONGO_URI": {
     "description": "Your Mongo url",
